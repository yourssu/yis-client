{
  "name": "yourssu-yis-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently 'vite' 'pnpm gen-color:watch'",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "prepare": "husky",
    "preview": "vite preview",
    "gen-color": "pnpm tsx ./scripts/codegen/gen-color.mts",
    "gen-color:watch": "pnpm tsx watch --clear-screen=false --include ./src/styles/color.variable.css ./scripts/codegen/gen-color.mts"
  },
  "dependencies": {
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-slot": "^1.2.0",
    "@tanstack/react-query": "^5.75.2",
    "@tanstack/react-router": "^1.119.0",
    "@tanstack/react-router-devtools": "^1.119.1",
    "clsx": "^2.1.1",
    "ky": "^1.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-simplikit": "^0.0.35",
    "tailwind-variants": "^1.0.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@tailwindcss/vite": "^4.1.5",
    "@tanstack/react-query-devtools": "^4.36.1",
    "@tanstack/router-plugin": "^1.119.0",
    "@types/node": "^22.15.3",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-perfectionist": "^4.12.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "19.1.0-rc.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.5",
    "tsx": "^4.19.4",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1",
    "vite-tsconfig-paths": "^5.1.4",
    "zx": "^8.5.3"
  },
  "lint-staged": {
    "*.{js,mjs,jsx,ts,mts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@10.10.0+sha256.fa0f513aa8191764d2b6b432420788c270f07b4f999099b65bb2010eec702a30"
}
